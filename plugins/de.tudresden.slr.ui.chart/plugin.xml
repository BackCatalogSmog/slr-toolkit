<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.views">
      <view
            class="helloworldchart.HelloWorldView"
            id="chart.view.helloworld"
            name="HelloWorldChart"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="helloworldchart.ShowChartHandler"
            id="de.tudresden.slr.ui.chart.showchart"
            name="Show Chart">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <command
               commandId="de.tudresden.slr.ui.chart.showchart"
               label="Show Chart"
               style="push">
         </command>
      </menuContribution>
   </extension>

</plugin>
