<?xml version="1.0" encoding="windows-1252"?>
<?eclipse version="3.0"?>
<plugin>
	<extension
		point="org.eclipse.ui.editors">
		<editor
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.XtextEditor"
			contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
			default="true"
			extensions="taxonomy"
			id="de.tudresden.slr.model.Taxanomy"
			name="Taxanomy Editor">
		</editor>
	</extension>
	<extension
		point="org.eclipse.ui.handlers">
		<handler
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclarationHandler"
			commandId="org.eclipse.xtext.ui.editor.hyperlinking.OpenDeclaration">
			<activeWhen>
				<reference
					definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
		<handler
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.handler.ValidateActionHandler"
			commandId="de.tudresden.slr.model.Taxanomy.validate">
		<activeWhen>
			<reference
					definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
			</reference>
		</activeWhen>
		</handler>
		<!-- copy qualified name -->
		<handler
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName">
			<activeWhen>
				<reference definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened" />
			</activeWhen>
		</handler>
		<handler
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedNameHandler"
			commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName">
			<activeWhen>
				<and>
					<reference definitionId="de.tudresden.slr.model.Taxanomy.XtextEditor.opened" />
					<iterate>
						<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
					</iterate>
				</and>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.core.expressions.definitions">
		<definition id="de.tudresden.slr.model.Taxanomy.Editor.opened">
			<and>
				<reference definitionId="isActiveEditorAnInstanceOfXtextEditor"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
						value="de.tudresden.slr.model.Taxanomy" 
						forcePluginActivation="true"/>
				</with>		
			</and>
		</definition>
		<definition id="de.tudresden.slr.model.Taxanomy.XtextEditor.opened">
			<and>
				<reference definitionId="isXtextEditorActive"/>
				<with variable="activeEditor">
					<test property="org.eclipse.xtext.ui.editor.XtextEditor.languageName" 
						value="de.tudresden.slr.model.Taxanomy" 
						forcePluginActivation="true"/>
				</with>		
			</and>
		</definition>
	</extension>
	<extension
			point="org.eclipse.ui.preferencePages">
		<page
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="de.tudresden.slr.model.Taxanomy"
			name="Taxanomy">
			<keywordReference id="de.tudresden.slr.model.ui.keyword_Taxanomy"/>
		</page>
		<page
			category="de.tudresden.slr.model.Taxanomy"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.syntaxcoloring.SyntaxColoringPreferencePage"
			id="de.tudresden.slr.model.Taxanomy.coloring"
			name="Syntax Coloring">
			<keywordReference id="de.tudresden.slr.model.ui.keyword_Taxanomy"/>
		</page>
		<page
			category="de.tudresden.slr.model.Taxanomy"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.templates.XtextTemplatePreferencePage"
			id="de.tudresden.slr.model.Taxanomy.templates"
			name="Templates">
			<keywordReference id="de.tudresden.slr.model.ui.keyword_Taxanomy"/>
		</page>
	</extension>
	<extension
			point="org.eclipse.ui.propertyPages">
		<page
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.preferences.LanguageRootPreferencePage"
			id="de.tudresden.slr.model.Taxanomy"
			name="Taxanomy">
			<keywordReference id="de.tudresden.slr.model.ui.keyword_Taxanomy"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<extension
		point="org.eclipse.ui.keywords">
		<keyword
			id="de.tudresden.slr.model.ui.keyword_Taxanomy"
			label="Taxanomy"/>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
	<command
			description="Trigger expensive validation"
			id="de.tudresden.slr.model.Taxanomy.validate"
			name="Validate">
	</command>
	<!-- copy qualified name -->
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	<command
			id="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName"
			categoryId="org.eclipse.ui.category.edit"
			description="Copy the qualified name for the selected element"
			name="Copy Qualified Name">
	</command>
	      <command
            defaultHandler="de.tudresden.slr.model.ui.handlers.CreateBubbleChartHandler"
            description="Create a BubbleChart for two taxonomy terms"
            id="de.tudresden.slr.model.taxonomy.ui.createbubblechart"
            name="Create BubbleChart">
      </command>
      <command
            defaultHandler="de.tudresden.slr.model.ui.handlers.CreateCiteHandler"
            id="de.tudresden.slr.model.taxonomy.ui.createcitebar"
            name="Create Cite Bar">
      </command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			 <command
				 commandId="de.tudresden.slr.model.Taxanomy.validate"
				 style="push"
				 tooltip="Trigger expensive validation">
			<visibleWhen checkEnabled="false">
				<reference
					definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
				</reference>
			</visibleWhen>
		</command>  
		</menuContribution>
		<!-- copy qualified name -->
		<menuContribution locationURI="popup:#TextEditorContext?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened" />
				</visibleWhen>
			</command>  
		</menuContribution>
		<menuContribution locationURI="menu:edit?after=copy">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.EditorCopyQualifiedName"
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<reference definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened" />
				</visibleWhen>
			</command>  
		</menuContribution>
		<menuContribution locationURI="popup:org.eclipse.xtext.ui.outline?after=additions">
			<command commandId="org.eclipse.xtext.ui.editor.copyqualifiedname.OutlineCopyQualifiedName" 
				style="push" tooltip="Copy Qualified Name">
				<visibleWhen checkEnabled="false">
					<and>
						<reference definitionId="de.tudresden.slr.model.Taxanomy.XtextEditor.opened" />
						<iterate>
							<adapt type="org.eclipse.xtext.ui.editor.outline.IOutlineNode" />
						</iterate>
					</and>
				</visibleWhen>
			</command>
		</menuContribution>
		<menuContribution
               allPopups="false"
               locationURI="toolbar:de.tudresden.slr.model.taxonomy.ui.views.TaxonomyCheckboxListView">
            <command
                  commandId="de.tudresden.slr.model.taxonomy.ui.createbubblechart"
                  label="Create Bubblechart"
                  style="push">
            </command>
            <command
                  commandId="de.tudresden.slr.model.taxonomy.ui.createcitebar"
                  label="Create Cite Barchart"
                  style="push">
            </command>
         </menuContribution>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?endof=group.find">
			<command commandId="org.eclipse.xtext.ui.editor.FindReferences">
				<visibleWhen checkEnabled="false">
					<reference definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.findrefs.FindReferencesHandler"
			commandId="org.eclipse.xtext.ui.editor.FindReferences">
			<activeWhen>
				<reference
					definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<!-- adding resource factories -->
	<extension
		point="org.eclipse.emf.ecore.extension_parser">
		<parser
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.resource.IResourceFactory"
			type="taxonomy">
		</parser>
	</extension>
	<extension point="org.eclipse.xtext.extension_resourceServiceProvider">
	    <resourceServiceProvider
	        class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.resource.IResourceUIServiceProvider"
	        uriExtension="taxonomy">
	    </resourceServiceProvider>
	</extension>
	<!-- marker definitions for de.tudresden.slr.model.Taxanomy -->
	<extension
			id="taxanomy.check.fast"
			name="Taxanomy Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.fast"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="taxanomy.check.normal"
			name="Taxanomy Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.normal"/>
		<persistent value="true"/>
	</extension>
	<extension
			id="taxanomy.check.expensive"
			name="Taxanomy Problem"
			point="org.eclipse.core.resources.markers">
		<super type="org.eclipse.xtext.ui.check.expensive"/>
		<persistent value="true"/>
	</extension>
	<extension point="org.eclipse.xtext.builder.participant">
		<participant
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.builder.IXtextBuilderParticipant"
			fileExtensions="taxonomy"/>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="de.tudresden.slr.model.Taxanomy"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="de.tudresden.slr.model.Taxanomy.compiler.preferencePage"
			name="Compiler">
			<keywordReference id="de.tudresden.slr.model.ui.keyword_Taxanomy"/>
		</page>
	</extension>
	<extension point="org.eclipse.ui.propertyPages">
		<page
			category="de.tudresden.slr.model.Taxanomy"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.builder.preferences.BuilderPreferencePage"
			id="de.tudresden.slr.model.Taxanomy.compiler.propertyPage"
			name="Compiler">
			<keywordReference id="de.tudresden.slr.model.ui.keyword_Taxanomy"/>
			<enabledWhen>
				<adapt type="org.eclipse.core.resources.IProject"/>
			</enabledWhen>
			<filter name="projectNature" value="org.eclipse.xtext.ui.shared.xtextNature"/>
		</page>
	</extension>
	<!-- new, not in old version -->
	<extension point="org.eclipse.ui.menus">
		<menuContribution locationURI="popup:#TextEditorContext?after=xtext.ui.openDeclaration">
			<command
				commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand"
				id="de.tudresden.slr.model.Taxanomy.OpenGeneratedCode"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened" />
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<!-- new, not in old version -->
	<extension point="org.eclipse.ui.handlers">
		<handler
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.generator.trace.OpenGeneratedFileHandler"
			commandId="org.eclipse.xtext.ui.OpenGeneratedFileCommand">
			<activeWhen>
				<reference definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened" />
			</activeWhen>
		</handler>
	</extension>
	<!-- Quick Outline -->
	<extension
		point="org.eclipse.ui.handlers">
		<handler 
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.outline.quickoutline.ShowQuickOutlineActionHandler"
			commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline">
			<activeWhen>
				<reference
					definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension
		point="org.eclipse.ui.commands">
		<command
			description="Open the quick outline."
			id="org.eclipse.xtext.ui.editor.outline.QuickOutline"
			name="Quick Outline">
		</command>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.open">
			<command commandId="org.eclipse.xtext.ui.editor.outline.QuickOutline"
				style="push"
				tooltip="Open Quick Outline">
				<visibleWhen checkEnabled="false">
					<reference definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened"/>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<!-- quickfix marker resolution generator for de.tudresden.slr.model.Taxanomy -->
	<extension
	        point="org.eclipse.ui.ide.markerResolution">
	    <markerResolutionGenerator
	        class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
	        markerType="de.tudresden.slr.model.taxanomy.ui.taxanomy.check.fast">
	        <attribute
	            name="FIXABLE_KEY"
	            value="true">
	        </attribute>
	    </markerResolutionGenerator>
	    <markerResolutionGenerator
	        class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
	        markerType="de.tudresden.slr.model.taxanomy.ui.taxanomy.check.normal">
	        <attribute
	            name="FIXABLE_KEY"
	            value="true">
	        </attribute>
	    </markerResolutionGenerator>
	    <markerResolutionGenerator
	        class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.quickfix.MarkerResolutionGenerator"
	        markerType="de.tudresden.slr.model.taxanomy.ui.taxanomy.check.expensive">
	        <attribute
	            name="FIXABLE_KEY"
	            value="true">
	        </attribute>
	    </markerResolutionGenerator>
	</extension>
	<!-- Rename Refactoring -->
	<extension point="org.eclipse.ui.handlers">
		<handler 
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.DefaultRenameElementHandler"
			commandId="org.eclipse.xtext.ui.refactoring.RenameElement">
			<activeWhen>
				<reference
					definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
				</reference>
			</activeWhen>
		</handler>
	</extension>
	<extension point="org.eclipse.ui.menus">
		<menuContribution
			locationURI="popup:#TextEditorContext?after=group.edit">
			<command commandId="org.eclipse.xtext.ui.refactoring.RenameElement"
				style="push">
				<visibleWhen checkEnabled="false">
					<reference
						definitionId="de.tudresden.slr.model.Taxanomy.Editor.opened">
					</reference>
				</visibleWhen>
			</command>
		</menuContribution>
	</extension>
	<extension point="org.eclipse.ui.preferencePages">
		<page
			category="de.tudresden.slr.model.Taxanomy"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.refactoring.ui.RefactoringPreferencePage"
			id="de.tudresden.slr.model.Taxanomy.refactoring"
			name="Refactoring">
			<keywordReference id="de.tudresden.slr.model.ui.keyword_Taxanomy"/>
		</page>
	</extension>
	<extension point="org.eclipse.compare.contentViewers">
		<viewer id="de.tudresden.slr.model.Taxanomy.compare.contentViewers"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="taxonomy">
		</viewer>
	</extension>
	<extension point="org.eclipse.compare.contentMergeViewers">
		<viewer id="de.tudresden.slr.model.Taxanomy.compare.contentMergeViewers"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.compare.InjectableViewerCreator"
			extensions="taxonomy" label="Taxanomy Compare">
		</viewer>
	</extension>
	<extension point="org.eclipse.ui.editors.documentProviders">
		<provider id="de.tudresden.slr.model.Taxanomy.editors.documentProviders"
			class="de.tudresden.slr.model.ui.TaxanomyExecutableExtensionFactory:org.eclipse.xtext.ui.editor.model.XtextDocumentProvider"
			extensions="taxonomy">
		</provider>
	</extension>
	<extension point="org.eclipse.team.core.fileTypes">
		<fileTypes
			extension="taxonomy"
			type="text">
		</fileTypes>
	</extension>
	<extension
        point="org.eclipse.ui.views">
     <category
           id="de.tudresden.slr.model.taxonomy.ui"
           name="Sample Category">
     </category>
     <view
           category="de.tudresden.slr.model.taxonomy.ui"
           class="de.tudresden.slr.model.ui.views.TaxonomyCheckboxListView"
           icon="icons/sample.gif"
           id="de.tudresden.slr.model.taxonomy.ui.views.TaxonomyCheckboxListView"
           name="Taxonomy Checkbox ListView">
     </view>
  </extension>
    <extension
        point="org.eclipse.help.contexts">
     <contexts
           file="contexts.xml">
     </contexts>
  </extension>
</plugin>
